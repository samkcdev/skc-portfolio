{
  "name": "skc-portfolio",
  "version": "1.0.0",
  "description": "Sam Cherians Personal Website",
  "main": "index.js",
  "scripts": {
    "watch:sass": "sass --watch src/assets/styles:_output/assets/styles",
    "build:sass": "sass src/assets/styles:_output/assets/styles",
    "watch:eleventy": "eleventy --serve",
    "build:eleventy": "eleventy",
    "start": "npm-run-all build:sass --parallel watch:*",
    "build": "npm-run-all build:sass --parallel build:*"
  },
  "keywords": [
    "Front-End Developer",
    "developer",
    "Web Developer",
    "Portfolio",
    "Eleventy"
  ],
  "author": "Sam Cherian",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-navigation": "^0.3.2"
  },
  "dependencies": {
    "npm-run-all": "^4.1.5",
    "sass": "^1.43.4"
  }
}
